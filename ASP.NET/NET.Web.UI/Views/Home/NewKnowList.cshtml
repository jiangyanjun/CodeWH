  @model IEnumerable<NET.Architect.Model.KnowledgeBase>

    @{
        ViewBag.Title = "List";
        Layout = "~/Views/Shared/_Layout.cshtml";
    }

    <div class="row">
        <div class="col-sm-12 col-md-12">
            <p>
                @Html.ActionLink("新建知识页面", "NewKnow")
                @Html.ActionLink("用户日志记录列表", "Index", "TruingLog")
                @Html.ActionLink("进入知识列表", "Index", "Home")
                @Html.ActionLink("进入用户页面", "Index", "UserInfo")
                @Html.ActionLink("进入聊天页面", "Chat", "Home")
            </p>
            <hr />

            <p>
                &nbsp;  &nbsp; &nbsp;
            </p>
            <hr />
        </div>
    </div>
    <div class="row">
        <div class="col-sm-12 col-md-12">
            <table>
                <tr class="table row">
                    <th class="col-sm-3 col-md-3">
                        @Html.DisplayNameFor(model => model.Info)
                    </th>
                    <th class="col-sm-3 col-md-3">
                        @Html.DisplayNameFor(model => model.Text)
                    </th>
                    <th class="col-sm-3 col-md-3">
                        @Html.DisplayNameFor(model => model.LAST_MODIFIED_TIME)
                    </th>
                    <th class="col-sm-3 col-md-3">    操    作   </th>
                </tr>
                @foreach (var item in Model)
            {
                    <tr class="table row">
                        <td class="col-sm-3 col-md-3">
                            @Html.DisplayFor(modelItem => item.Info)
                        </td>
                        <td class="col-sm-3 col-md-3">
                            @Html.DisplayFor(modelItem => item.Text)
                        </td>
                        <td class="col-sm-3 col-md-3">
                            @Html.DisplayFor(modelItem => item.LAST_MODIFIED_TIME)
                        </td>
                        <td class="col-sm-3 col-md-3">
                            @Html.ActionLink("Edit", "Edit", new { id = item.Info }) |
                            @Html.ActionLink("Details", "Details", new { id = item.Info }) |
                            @Html.ActionLink("Delete", "Delete", new { id = item.Info })
                        </td>
                    </tr>
                }

            </table>
        </div>
    </div>
<div>
    @Html.ActionLink("Back to List", "Chat", "Home")&nbsp;  &nbsp; &nbsp;@Html.ActionLink("TruingLog List", "List", "TruingLog")
</div>