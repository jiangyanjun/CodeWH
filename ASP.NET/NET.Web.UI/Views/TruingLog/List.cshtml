            @Html.DisplayNameFor(model => model.LAST_MODIFIED_TIME)
        </th>
        <th class="col-sm-3 col-md-3">
            @Html.DisplayNameFor(model => model.IP)
        </th>
        <th class="col-sm-3 col-md-3">
